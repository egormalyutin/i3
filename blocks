full_text=<
align=center
color=#666666
separator=false
separator_block_width=3

[music]
label=
command=cat ~/cmus-status.txt
interval=10
color=#FFFFFF

[separator]

[CPU]
label=
command=~/.config/i3/scripts/cpu_usage
interval=2
color=#FFFFFF

[separator]

[memory]
label=
command=~/.config/i3/scripts/memory | sed -n 2p
interval=5
color=#FFFFFF

[separator]

[battery]
label=⚡
command=~/.config/i3/scripts/battery | sed -n 2p
interval=20
color=#FFFFFF

[separator]

[volume]
label= 
command=amixer get Master | grep -E -o '[0-9]{1,3}?%' | head -1
interval=1
signal=1
color=#FFFFFF

[separator]

[date]
label= 
command=date '+%D'
interval=60
color=#FFFFFF

[separator]

[time]
label= 
command=date '+%T'
interval=60
color=#FFFFFF


[separator]

[user]
command=echo $USER@$HOSTNAME
interval=once
color=#FFFFFF
