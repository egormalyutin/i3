#!/bin/bash

paused=$(cmus-remote -Q | grep 'status paused')
playing=$(cmus-remote -Q | grep 'status playing')
stopped=$(cmus-remote -Q | grep 'status stopped')
working=$(cmus-remote -Q | grep 'status')

if [[ working ]]
then
	if [[ stopped ]]
	then
		echo ""
	fi

	if [[ paused ]]
	then
		echo ""
	fi

	if [[ playing ]]
	then
		echo ""
	fi
fi
